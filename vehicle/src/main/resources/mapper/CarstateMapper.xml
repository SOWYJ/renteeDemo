<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.vehicle.mapper.CarstateMapper">

    <resultMap id="BaseResultMap" type="com.example.vehicle.domain.Carstate">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="hourPrice" column="hour_price" jdbcType="DECIMAL"/>
            <result property="dropLocation" column="drop_location" jdbcType="VARCHAR"/>
            <result property="dropDate" column="drop_date" jdbcType="TIMESTAMP"/>
            <result property="carstatus" column="car_status" jdbcType="OTHER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,hour_price,drop_location,
        drop_date,carstatus
    </sql>
</mapper>
